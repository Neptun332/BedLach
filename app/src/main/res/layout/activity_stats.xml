<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.StatsActivity">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="100dp"
        android:layout_marginTop="15dp"
        android:text="Count" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="134dp"
        android:text="Sum of Area" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="-1dp"
        android:text="Sum of param" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="58dp"
        android:text="Circle" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView5"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-3dp"
        android:layout_marginTop="99dp"
        android:text="Square" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView5"
        android:layout_alignParentTop="true"
        android:layout_marginTop="149dp"
        android:layout_marginEnd="-8dp"
        android:text="Triangle" />

    <TextView
        android:id="@+id/CircleCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignParentStart="true"
        android:layout_marginStart="89dp"
        android:text="TextView"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/CircleSumofArea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="142dp"
        android:text="TextView"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/CircleSumofParameter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="35dp"
        android:text="TextView"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/SquareCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/CircleCount"
        android:layout_alignTop="@+id/textView6"
        android:text="TextView"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/SquareSumofArea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/CircleSumofArea"
        android:layout_alignTop="@+id/textView6"
        android:text="TextView"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/SquareSumofParameter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/CircleSumofParameter"
        android:layout_alignTop="@+id/textView6"
        android:text="TextView"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/TriangleCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/CircleCount"
        android:layout_alignTop="@+id/textView7"
        android:text="TextView"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/TriangleSumofArea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/CircleSumofArea"
        android:layout_alignTop="@+id/textView7"
        android:text="TextView"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/TriangleSumofParameter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/CircleSumofParameter"
        android:layout_alignTop="@+id/textView7"
        android:text="TextView"
        android:textAlignment="center" />

</RelativeLayout>